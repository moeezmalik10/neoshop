<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 32 32" width="32" height="32">
  <defs>
    <!-- Shopping bag gradient -->
    <linearGradient id="bagGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#FFA500;stop-opacity:1" />
      <stop offset="50%" style="stop-color:#FF4500;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#8B0000;stop-opacity:1" />
    </linearGradient>
    
    <!-- Blue gradient for handles -->
    <linearGradient id="handleGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#4169E1;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#0000CD;stop-opacity:1" />
    </linearGradient>
    
    <!-- Background gradient -->
    <linearGradient id="bgGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#1a1a2e;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#16213e;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- Background circle -->
  <circle cx="16" cy="16" r="15" fill="url(#bgGradient)" stroke="#ffffff" stroke-width="0.5"/>
  
  <!-- Speed lines (left side) -->
  <path d="M4 8 L8 10" stroke="#87CEEB" stroke-width="1.5" stroke-linecap="round"/>
  <path d="M3 12 L7 14" stroke="#FFD700" stroke-width="1.5" stroke-linecap="round"/>
  <path d="M4 16 L8 18" stroke="#FF6347" stroke-width="1.5" stroke-linecap="round"/>
  
  <!-- Speed lines (right side) -->
  <path d="M24 8 L28 10" stroke="#FF6347" stroke-width="1.5" stroke-linecap="round"/>
  <path d="M25 12 L29 14" stroke="#87CEEB" stroke-width="1.5" stroke-linecap="round"/>
  <path d="M24 16 L28 18" stroke="#FFD700" stroke-width="1.5" stroke-linecap="round"/>
  
  <!-- Shopping bag main body -->
  <path d="M10 12 L22 12 L22 24 C22 25.1 21.1 26 20 26 L12 26 C10.9 26 10 25.1 10 24 Z" fill="url(#bagGradient)"/>
  
  <!-- Shopping bag handles -->
  <path d="M12 12 L12 8 C12 6.9 12.9 6 14 6 L18 6 C19.1 6 20 6.9 20 8 L20 12" stroke="url(#handleGradient)" stroke-width="2" fill="none" stroke-linecap="round"/>
  
  <!-- Cursor icon on bag -->
  <circle cx="16" cy="18" r="2.5" fill="#ffffff"/>
  <path d="M16 18 L18 16 L17 17 L19 17 L17 19 Z" fill="#FFA500"/>
  
  <!-- Bag opening -->
  <path d="M10 12 L22 12" stroke="#ffffff" stroke-width="1" stroke-linecap="round"/>
</svg>